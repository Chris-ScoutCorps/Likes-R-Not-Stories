{
  "manifest_version": 2,

  "name": "Likes are not Stories",
  "description": "Removes 'X liked Y' stories from the Facebook news feed",
  "version": "1.0",

  "icons": {
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*", "*://facebook.com/*"],
      "js": [
        "lib/jquery-3.2.1.min.js",
        "util/aggregate.js",
        "sites/facebook.js"
      ]
    }
  ]
}